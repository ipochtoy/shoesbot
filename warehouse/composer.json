{
    "name": "warehouse/telegram-bot",
    "description": "Telegram bot for barcode scanning with Pochtoy integration",
    "type": "project",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0",
        "telegram-bot/api": "^3.0",
        "guzzlehttp/guzzle": "^7.8",
        "intervention/image": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
